    <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>Ichor_csharp</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="lib\ichor.dll" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="lib\ichor.dll">
